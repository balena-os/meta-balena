[Unit]
Description=Prepare OpenVPN Application On Resin
Requires=mnt-boot.mount resin-device-uuid.service resin-device-api-key.service var-volatile.mount
After=mnt-boot.mount resin-device-uuid.service resin-device-api-key.service var-volatile.mount

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=@BINDIR@/prepare-openvpn

[Install]
WantedBy=resin.target
