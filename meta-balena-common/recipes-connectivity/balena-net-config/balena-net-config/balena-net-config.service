[Unit]
Description=Resin network configure service
Requires=bind-etc-NetworkManager-system-connections.service
After=bind-etc-NetworkManager-system-connections.service

[Service]
ExecStart=@BASE_BINDIR@/bash @BINDIR@/balena-net-config
Type=oneshot
RemainAfterExit=yes
StandardError=journal+console

[Install]
WantedBy=multi-user.target
