[Unit]
Description=udev rules configuration from config.json
Requires=systemd-udev-settle.service bind-etc-udev-rules.d.service
After=systemd-udev-settle.service bind-etc-udev-rules.d.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=@SBINDIR@/os-udevrules

[Install]
WantedBy=multi-user.target
