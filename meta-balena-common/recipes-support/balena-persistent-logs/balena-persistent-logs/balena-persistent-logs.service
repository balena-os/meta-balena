[Unit]
Description=Balena persistent logs
Requires=resin-boot.service mnt-state.mount systemd-journald.service
After=resin-boot.service mnt-state.mount systemd-journald.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=@BINDIR@/balena-persistent-logs

[Install]
WantedBy=multi-user.target
