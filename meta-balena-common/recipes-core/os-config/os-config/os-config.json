{
	"services": [
		{
			"id": "openvpn",
			"files": {
				"config": {
					"path": "/etc/openvpn/openvpn.conf",
					"perm": ""
				},
				"ca": {
					"path": "/etc/openvpn/ca.crt",
					"perm": ""
				}
			},
			"systemd_services": ["prepare-openvpn.service", "openvpn.service"]
		},
		{
			"id": "ssh",
			"files": {
				"authorized_keys": {
					"path": "/home/root/.ssh/authorized_keys_remote",
					"perm": "600"
				}
			},
			"systemd_services": []
		}
	],
	"keys": [
		"applicationId",
		"deviceType",
		"appUpdatePollInterval",
		"listenPort",
		"apiEndpoint",
		"registryEndpoint",
		"deltaEndpoint",
		"mixpanelToken",
		"apiKey",
		"version",
		"deviceApiKey",
		"registered_at",
		"deviceId",
		"balenaRootCA"
	],
	"config": {
		"whitelist": [
			"logsEndpoint"
		]
	}
}
